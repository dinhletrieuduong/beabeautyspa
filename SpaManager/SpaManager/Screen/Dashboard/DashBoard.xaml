<UserControl x:Class="SpaManager.Screen.Dashboard.DashBoard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SpaManager.Screen.Dashboard"
        mc:Ignorable="d"
         Height="650" Width="1280">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <StackPanel Name="st_menu" Height="646" Width="200" HorizontalAlignment="Left" Background="Green" Margin="0 0 0 0">

            </StackPanel>

            <StackPanel Height="620" Width="1070" VerticalAlignment="Top">
                <StackPanel x:Name="st_bar" Height="50" Width="1070"/>

                <StackPanel Width="1070" Height="1.5" Background="Green" Margin="0 15 0 0"/>


                <StackPanel Height="550" Width="1070" VerticalAlignment="Center" Name="main_page">


                </StackPanel>

            </StackPanel>
        </StackPanel>
    </Grid>
</UserControl>
